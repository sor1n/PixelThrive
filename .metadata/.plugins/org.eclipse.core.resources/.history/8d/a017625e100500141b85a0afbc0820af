package com.base.pixelthrive;

import com.base.engine.CoreEngine;
import com.base.engine.Game;

public class PTGame extends Game
{
	public PTGame()
	{
		super("PixelThrive");
	}
	
	@Override
	public void init()
	{
		super.init();
		addObject(new World(new Vector2f(50, 60), 0xCEF3FF));
	}
	
	public static class BIQMain
	{
		public static void main(String[] args)
		{
			CoreEngine engine = new CoreEngine(new Vector2f(800, 600), 60D, new BIQGame());
			engine.start();
		}
	}
}
