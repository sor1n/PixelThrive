package com.base.pixelthrive;

import com.base.engine.CoreEngine;
import com.base.engine.Game;
import com.base.engine.GameObject;
import com.base.engine.Vector2f;

public class PTGame extends Game
{
	public PTGame()
	{
		super("PixelThrive");
	}
	
	@Override
	public void init()
	{
		super.init();
		addObject();
	}
	
	public static class PTMain
	{
		public static void main(String[] args)
		{
			CoreEngine engine = new CoreEngine(new Vector2f(800, 600), 60D, new PTGame());
			engine.start();
		}
	}
}
